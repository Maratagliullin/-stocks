AAW;AAC1C,WAAOA,KAAK,CAACK,EAAN,KAAaA,EAApB;AACD,GAFa,CAAd;AAGD,CAlBY,+BAoBZT,SApBY,YAoBDG,KApBC,SAoBe;AAAA,MAARC,KAAQ,SAARA,KAAQ;AAC1BD,EAAAA,KAAK,CAACC,KAAN,GAAcA,KAAd;AACD,CAtBY,cAAf,C,CAyBA;;AACA,IAAMO,OAAO,GAAG;AACdC,EAAAA,WADc,8BACQC,SADR,EACmB;AAAA,QAApBC,MAAoB,SAApBA,MAAoB;AAC/BA,IAAAA,MAAM,CAACjB,YAAD,EAAe,IAAf,CAAN;AACAI,IAAAA,KAAK,CAACc,MAAN,CAAaF,SAAb,EAAwBG,IAAxB,CAA6B,UAACZ,KAAD,EAAW;AACtCU,MAAAA,MAAM,CAAClB,SAAD,EAAYQ,KAAZ,CAAN;AACAU,MAAAA,MAAM,CAACP,aAAD,EAAgB,KAAhB,CAAN;AACD,KAHD;AAKD,GARa;AASdU,EAAAA,WATc,8BASQb,KATR,EASe;AAAA,QAAhBU,MAAgB,SAAhBA,MAAgB;AAC3Bb,IAAAA,KAAK,CAACiB,MAAN,CAAad,KAAb,EAAoBY,IAApB,CAAyB,UAACZ,KAAD,EAAW;AAClCU,MAAAA,MAAM,CAACf,YAAD,EAAeK,KAAf,CAAN;AACD,KAFD;AAGD,GAba;AAcde,EAAAA,QAdc,2BAcK;AAAA,QAATL,MAAS,SAATA,MAAS;AACjBb,IAAAA,KAAK,CAACD,SAAN,GAAkBgB,IAAlB,CAAuB,UAACZ,KAAD,EAAW;AAChCU,MAAAA,MAAM,CAACd,SAAD,EAAY;AAACI,QAAAA,KAAK,EAALA;AAAD,OAAZ,CAAN;AACD,KAFD;AAGD;AAlBa,CAAhB;AAqBA,eAAe,IAAIT,IAAI,CAACyB,KAAT,CAAe;AAC5BjB,EAAAA,KAAK,EAALA,KAD4B;AAE5BQ,EAAAA,OAAO,EAAPA,OAF4B;AAG5BL,EAAAA,SAAS,EAATA;AAH4B,CAAf,CAAf","sourcesContent":["import Vue from 'vue'\nimport Vuex from 'vuex'\nimport {add_stock, loading_true,loading_, remove_stock, get_stock} from './mutation-types.js'\nimport {Stock} from '../api/stock'\n\nVue.use(Vuex)\n\n// Состояние\nconst state = {\n  stock: [], // список акций\n  loader: false, // loader\n}\n\n\n// Мутации\nconst mutations = {\n  [loading_true](state) {\n    state.loader = true\n  },\n  [loading_false](state) {\n    state.loader = false\n  },\n  // Добавляем тикер в список\n  [add_stock](state, stock) {\n    if (stock != undefined) {\n      state.stock = stock\n    }\n  },\n  // Убираем тикер из списка\n  [remove_stock](state, {id}) {\n    state.stock = state.stock.filter((stock) => {\n      return stock.id !== id\n    })\n  },\n  // Задаем список тикеров\n  [get_stock](state, {stock}) {\n    state.stock = stock\n  },\n}\n\n// Действия\nconst actions = {\n  createStock({commit}, stockData) {\n    commit(loading_true, true)\n    Stock.create(stockData).then((stock) => {\n      commit(add_stock, stock)\n      commit(loading_false, false)\n    })\n   \n  },\n  deleteStock({commit}, stock) {\n    Stock.delete(stock).then((stock) => {\n      commit(remove_stock, stock)\n    })\n  },\n  getStock({commit}) {\n    Stock.get_stock().then((stock) => {\n      commit(get_stock, {stock})\n    })\n  },\n}\n\nexport default new Vuex.Store({\n  state,\n  actions,\n  mutations,\n})\n"]}]}